{
    "name": "torrent-chain",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:Firaenix/TorrentChain.git",
    "author": "Firaenix <nick.carton@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "webpack-dev-server -d",
        "dist": "webpack -p",
        "server-start": "dotnet run --project  ./TorrentChain.Web/TorrentChain.Web.csproj",
        "prep-grpc-tools":
            "chmod +x ./Grpc.Tools.1.11.0/tools/linux_x64/protoc && chmod +x ./Grpc.Tools.1.11.0/tools/linux_x64/grpc_csharp_plugin",
        "run-grpc":
            "./Grpc.Tools.1.11.0/tools/linux_x64/protoc -I ./TorrentChain.Service/Contracts --csharp_out ./TorrentChain.Service/Contracts --grpc_out ./TorrentChain.Service/Contracts ./TorrentChain.Service/Contracts/BlockSyncService.proto --plugin=protoc-gen-grpc=./Grpc.Tools.1.11.0/tools/linux_x64/grpc_csharp_plugin",
        "run-grpc-win":
            "./Grpc.Tools.1.11.0/tools/windows_x64/protoc.exe -I ./TorrentChain.Service/Contracts --csharp_out ./TorrentChain.Service/Contracts --grpc_out ./TorrentChain.Service/Contracts ./TorrentChain.Service/Contracts/BlockSyncService.proto --plugin=protoc-gen-grpc=./Grpc.Tools.1.11.0/tools/windows_x64/grpc_csharp_plugin.exe"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "css-loader": "^0.28.11",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "redux-zero": "^4.12.0",
        "style-loader": "^0.20.3",
        "tslint": "^5.9.1",
        "tslint-loader": "^3.6.0",
        "uglifyjs-webpack-plugin": "^1.2.4"
    },
    "devDependencies": {
        "@types/react": "^16.3.10",
        "@types/react-dom": "^16.0.5",
        "core-js": "^2.5.5",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "node-sass": "^4.8.3",
        "postcss-loader": "^2.1.3",
        "promise-polyfill": "^7.1.2",
        "raf": "^3.4.0",
        "sass-loader": "^7.0.1",
        "ts-loader": "^4.2.0",
        "tslint-config-prettier": "^1.10.0",
        "tslint-react": "^3.5.1",
        "typescript": "^2.8.1",
        "webpack": "^4.5.0",
        "webpack-cli": "^2.0.14",
        "webpack-dev-server": "^3.1.3",
        "write-file-webpack-plugin": "^4.2.0"
    }
}
